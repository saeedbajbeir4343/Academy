    "version": "0.2.0",
    "configurations": [
    

        {
            "name": "Run Odoo",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "console": "integratedTerminal",
            "python": "/usr/bin/python3", // cari dengan which python3
            "program": "/Home/masa/odoo/odoo-14.0/odoo-bin",
            "args": [
                "--config=/Home/masa/odoo/config/odoo.conf",
            ]
        },

        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        }
    ]
}