    "version": "0.2.0",
    "configurations": [
        {
            "type": "python",
            "name": "Run Odoo",
            "request": "launch",
            "stopOnEntry": false,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "python": "/usr/bin/python3", // cari dengan which python3
            "args": [
                "--config=/home/masa/odoo/config/openacademy.conf",
                "-u","opencademy",
                
            ],
            "program": "/home/masa/odoo/odoo-14.0/odoo-bin",
            
        },
       
        {
            "type": "pwa-msedge",
            "request": "launch",
            "name": "Launch Edge against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}